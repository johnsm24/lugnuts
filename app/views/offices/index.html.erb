<p id="notice"><%= notice %></p>

<h1>Offices</h1>

<table>
  <thead>
    <tr>
      <th>City</th>
      <th>Phone</th>
      <th>Addressline1</th>
      <th>Addressline2</th>
      <th>State</th>
      <th>Country</th>
      <th>Postalcode</th>
      <th>Territory</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @offices.each do |office| %>
      <tr>
        <td><%= office.city %></td>
        <td><%= office.phone %></td>
        <td><%= office.addressLine1 %></td>
        <td><%= office.addressLine2 %></td>
        <td><%= office.state %></td>
        <td><%= office.country %></td>
        <td><%= office.postalCode %></td>
        <td><%= office.territory %></td>
        <td><%= link_to 'Show', office %></td>
        <td><%= link_to 'Edit', edit_office_path(office) %></td>
        <td><%= link_to 'Destroy', office, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Office', new_office_path %>
